<h1>RAMENTAROU</h1>
<h3>新規投稿ページ</h3>
<%= form_for @tweet do |f| %>

  <div class="field">
    <%= f.label :store %>
    <%= f.text_field :store, :size => 30 %>
  </div>

  <div class="field">
  <%= f.label :product %>
  <%= f.text_field :product, :size => 30 %>
</div>

<div class="field">
<%= f.label :about %>
<%= f.text_area :about, :size => 30 %>
</div>

<div class="field">
<%= f.label :image %>
<%= f.file_field :image %>
</div>

<%= f.submit "投稿する" %>
<% end %>

<%= link_to "一覧に戻る", tweets_path %>